{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Dimension Specification",
    "required": [
        "schoolId",
        "district",
        "block"
    ],
    "properties": {
        "schoolId": {
            "type": "string",
            "default": "",
            "title": "The schoolId schema",
            "examples": [
                "1"
            ]
        },
        "district": {
            "type": "string",
            "default": "",
            "title": "The district schema",
            "examples": [
                "Shimla"
            ]
        },
        "block": {
            "type": "string",
            "default": "",
            "title": "The block schema",
            "examples": [
                "Tikkar"
            ]
        }
    },
    "examples": [
        {
            "schoolId": "1",
            "district": "Shimla",
            "block": "Tikkar"
        }
    ]
}
